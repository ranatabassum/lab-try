
@{
    ViewBag.Title = "CatList";
}

<h2>Category List</h2>

<table class="table">

    <tr>
        <th>
            ID
        </th>
        <th>
            Name
        </th>
    

    </tr>



    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.ID
            </td>
            <td>
               
                <a href="/Home/Edit/@item.ID">Edit</a>
            </td>

        </tr>
    }


</table>